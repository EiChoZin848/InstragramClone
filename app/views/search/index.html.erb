<h1>Search#index</h1>
<p>Find me in app/views/search/index.html.erb</p>
<div class="search-page">
  <% if @posts.exists? %>
    <h1>Top Posts</h1>
  <% else %>
    <h1>Oop! No matching posts ...</h1>
  <% end %>
<div class="user-images">
    <% @posts.each do |post|%>
      <div class="wrapper">
        <%= link_to (image_tag post.image), post_path(post) %>
      </div>
    <% end %>
  </div>
</div>